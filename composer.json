{
    "name": "dynamic/recipe-silverstripe-base-site",
    "description": "A kitchen sink install of SilverStripe Base Site.",
    "type": "silverstripe-recipe",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "https://www.dynamicagency.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "axllent/silverstripe-bootstrap-forms": "^2.0",
        "axllent/silverstripe-version-truncator": "^2.0",
        "dynamic/silverstripe-base-site": "^5.0",
        "dynamic/recipe-silverstripe-elemental-base-site": "^1.0",
        "fractas/elemental-stylings": "^1.0@RC",
        "silverstripe/elemental-bannerblock": "^2.0",
        "silverstripe/recaptcha": "^3.0",
        "silverstripe/redirectedurls": "^2.0",
        "silverstripe/securityreport": "^2.0",
        "silverstripe/spamprotection": "^3.0",
        "silverstripe/widgets": "^2.0",
        "tractorcow/silverstripe-robots": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "phpmd/phpmd": "^2.6"
    },
    "suggest": {
        "ryanpotter/silverstripe-cms-theme": "Allows for customizing colors, admin groups, and admin icons in the CMS",
        "innoweb/silverstripe-social-metadata": "Meta module that helps build social and schema information for pages"
    },
    "config": {
        "process-timeout": 600
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\BaseRecipe\\": "src/",
            "Dynamic\\BaseRecipe\\Test\\": "tests/"
        }
    },
    "extra": {
        "project-files": ["app/_config/*"],
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
